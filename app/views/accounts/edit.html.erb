<%= turbo_frame_tag dom_id(account) do %>
  <%= form_with model: account, builder: MaterialFormBuilder, data: { controller: 'material-form' } do |f| %>
    <%= f.text_field :name %>
    <%= f.number_field :balance  %>
    <%= f.button "Submit", type:"submit", class: "mdc-button--raised" %>
  <% end %>
<% end %>
